{
    "compilerOptions": {
      "lib": [
        "ESNext",
        "DOM",
        "DOM.Iterable"
      ],
      "module": "esnext",
      "target": "esnext",
      "moduleResolution": "bundler",
      "strict": true,
      "downlevelIteration": true,
      "skipLibCheck": true,
      "allowSyntheticDefaultImports": true,
      "forceConsistentCasingInFileNames": true,
      "allowJs": true,
      "noEmit": true,
      "types": []
    },
    "include": ["./assets/"],
    "exclude": []
  }